{
    "name": "repair-quoter",
    "version": "5.3.1",
    "description": "repair-quoter is a planned replacement of the Phone Doctors 2021 repair quote implementation. ",
    "private": false,
    "type": "module",
    "main": "api/main.js",
    "scripts": {
        "start": "node api/main.js",
        "build": "prettier --write ./api/ && eslint --fix ./api/",
        "format:check": "prettier --check ./api/",
        "format:write": "prettier --write ./api/",
        "lint:check": "eslint ./api/",
        "lint:fix": "eslint --fix ./api/",
        "serve:client": "serve ./client/",
        "serve:api": "npm start"
    },
    "author": "Kyle Berry",
    "repository": {
        "type": "git",
        "url": "https://github.com/berrytechnics/repair-quoter"
    },
    "license": "GPL-3.0-only",
    "dependencies": {
        "bcryptjs": "2.4.3",
        "body-parser": "1.20.1",
        "cors": "2.8.5",
        "ejs": "3.1.8",
        "express": "4.18.2",
        "express-session": "1.17.3",
        "html-to-text": "8.2.1",
        "jsonwebtoken": "8.5.1",
        "mongoose": "6.7.2",
        "mongoose-paginate-v2": "1.7.1",
        "node-schedule": "2.1.0",
        "nodemailer": "6.8.0"
    },
    "devDependencies": {
        "chalk": "5.1.2",
        "dotenv": "16.0.3",
        "eslint": "8.28.0",
        "eslint-config-prettier": "8.5.0",
        "nodemon": "2.0.20",
        "prettier": "2.7.1",
        "serve": "14.1.2"
    }
}
